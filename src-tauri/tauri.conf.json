{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "projeto-teste",
  "version": "0.1.0",
  "identifier": "com.projeto-teste.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../server/frontend-dist"
  },
  "app": {
    "windows": [
      {
        "title": "Mercadinho Pre√ßo Bom",
        "width": 500,
        "height": 700,
        "resizable": false
      }
    ],
    "security": {
      "csp": "default-src self http://127.0.0.1:3000"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
